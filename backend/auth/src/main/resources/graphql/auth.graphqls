type Query {
	me: AuthPayloadOrError!
}

type Mutation {
	refresh: RefreshResult!
}

type AuthPayload {
	email: String!
	name: String
}

type AuthError {
	message: String!
}

union AuthPayloadOrError = AuthPayload | AuthError

type RefreshResult {
	accessToken: String
	refreshToken: String
	message: String!
}
